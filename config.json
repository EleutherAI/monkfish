{
    "seed": 42,
    "backend": "tpu",
    "gcp":{
        "gcp_credentials_path": "",
        "gcp_bucket_name": "lvd_test"
    },
    "s3":{},
    "cpu":{},
    "gpu": {},
    "tpu":{
        "tpu_name": "greenland"
    },
    "diffusion_auto_encoder":{
        "resolution": [512, 256],
        "dist_manager":{
            "mesh_shape": [8,1,1]
        },
        "data_loader": {
            "fs_type": "local",
            "data_root_directory": "../dummy_image_data",
            "workers_per_node": 1,
            "batch_size": 32,
            "queue_depth": 10
        },
        "model": {
            "encoder":{
                "k":4,
                "n_layers": 3
            },
            "decoder":{
                "k":5,
                "n_layers": 3
            }
        },
        "train": {
            "lr":0.0001,
            "warmup_steps": 1000,
            "ckpt_freq": 5,
            "log_freq": 50,
            "total_steps": 10000
        },
        "checkpoints": {
            "fs_type": "local",
            "ckpt_root_directory": "../checkpoints"
        }
    },
    "transformer_ardm":{
        "dist_manager":{
            "mesh_shape": [8,1,1]
        },
        "data_loader": {
            "fs_type": "local",
            "data_root_directory": "../dummy_latent_data",
            "workers_per_node": 1,
            "batch_size": 32,
            "queue_depth": 10
        },
        "model": {
            "res_dim": 512,
            "io_dim": 512,
            "vocab": 256,
            "n_layers": 2,
            "n_head": 8,
            "mlp_dim": 512,
            "qk_dim": 128,
            "v_dim": 128 
        },
        "train": {
            "lr":0.0001,
            "warmup_steps": 1000,
            "ckpt_freq": 5,
            "log_freq": 50,
            "total_steps": 10000
        },
        "checkpoints": {
            "fs_type": "local",
            "ckpt_root_directory": "../checkpoints"
        }
    }
}
